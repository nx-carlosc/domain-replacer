{
  "manifest_version": 3,
  "name": "Domain replacer",
  "description": "Replace the domain for another one",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
   ]
}

{
  "manifest_version": 3,
  "name": "nx-developers-tools",
  "description": "Tools for nexian developers",
  "version": "0.1.2",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://jira.nexum.de/*"
      ],
      "js": [
        "jira-script.js"
      ]
    }
  ]
}
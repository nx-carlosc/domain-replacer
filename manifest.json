{
  "manifest_version": 3,
  "name": "domain-replacer",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["utils/cookies.js", "index.js"],
      "styles": "styles.css"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  }
}
